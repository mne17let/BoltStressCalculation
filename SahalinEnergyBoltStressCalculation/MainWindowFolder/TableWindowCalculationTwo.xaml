<Window x:Class="SahalinEnergyBoltStressCalculation.MainWindowFolder.TableWindowCalculationTwo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SahalinEnergyBoltStressCalculation.MainWindowFolder"
        mc:Ignorable="d"
        Title="TableWindowCalculationTwo" Height="400" Width="800"
        MinHeight="400" MinWidth="800">
    <Grid>
        <ScrollViewer>
            <StackPanel>
                <TextBlock FontSize="15" TextWrapping="Wrap" Padding="10">
                    <TextBlock Foreground="White" Background="Red" FontWeight="Bold" Padding="3">
                        Warning!
                    </TextBlock>
                    Target assembly gasket stress shall be below Maximum permissible gasket stress and shall
                    be above Minimum gasket seating stress/Minimum gasket operating stress
                </TextBlock>
                <Image Source="pack://application:,,,/Images/table4.jpg" RenderOptions.BitmapScalingMode="HighQuality"/>
            </StackPanel>

        </ScrollViewer>
    </Grid>
</Window>
